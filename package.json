{
  "name": "Bash",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "scripts": {
    "start": "cross-env NODE_ENV=local webpack-dev-server --config ./config/webpack.config.js --progress --color",
    "build": "webpack --config ./config/webpack.config.js",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": "tslint -c tslint.staged.json -p tsconfig.json -t verbose"
  },
  "dependencies": {
    "@sentry/browser": "^5.11.1",
    "@types/node": "^12.12.26",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@types/react-router-dom": "^4.3.5",
    "axios": "^0.19.2",
    "computed-async-mobx": "^4.2.1",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.4.2",
    "dayjs": "^1.8.19",
    "favicons-webpack-plugin": "^0.0.9",
    "file-loader": "^4.3.0",
    "html-webpack-plugin": "^3.2.0",
    "mobx": "^5.15.4",
    "mobx-react": "^6.1.4",
    "mobx-utils": "^5.5.3",
    "raw-loader": "^4.0.0",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "reflect-metadata": "^0.1.13",
    "remove-webpack-plugin": "^1.2.2",
    "sanitize.css": "^11.0.0",
    "serializr": "^1.5.4",
    "style-loader": "^0.23.1",
    "styled-components": "^4.4.1",
    "ts-loader": "^6.2.1",
    "typescript": "^3.7.5",
    "typescript-ioc": "^1.2.5",
    "typescript-plugin-styled-components": "^1.4.4",
    "url-loader": "^2.3.0",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  },
  "devDependencies": {
    "@types/jest": "^25.1.0",
    "@types/styled-components": "^4.4.2",
    "cross-env": "^7.0.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "lint-staged": "^10.0.4",
    "thread-loader": "^2.1.3",
    "tslint": "^6.0.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.2.0",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-dev-server": "^3.10.1"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.(ts|tsx)$": "./config/test-preprocessor.js"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "testMatch": [
      "**/tests/**/*.spec.ts?(x)"
    ]
  }
}